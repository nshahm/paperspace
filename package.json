  "main": "index.js",
  "scripts": {
    "post-install" : "typings install",  
    "test": "echo \"Error: no test specified\" && exit 1",
    "devserver": "./node_modules/.bin/webpack-dev-server --inline --hot --progress --colors --config webpack.server.config.js --content-base ./app",
